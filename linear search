#include <stdio.h>
int main()
{
int arr[20],size,key,index,i;
printf("number of elements in the list");
scanf("%d",&size);

printf("enter elements of the list");
for (i=0;i<size;i++)
scanf("%d",&arr[i]);
printf("enter the elements of search ie.key element:");
scanf("%d",&key);
for(index=0;index<size;index++);
if (arr[index]==key)
if(index<key)
printf("key element found at index %d",index);
else
printf("key element not found");
return 0;
}
